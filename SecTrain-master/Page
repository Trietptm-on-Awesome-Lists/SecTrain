    <script type="text/javascript">
  (function(){
        if(window.location.href  == 'https://corporate.walmart.com/shareholders2018') {
           window.location.href = 'https://corporate.walmart.com/shareholders';
        }
        else if(window.location.href  == 'https://corporate.walmart.com/walmart-2018-open-call-event') {
           window.location.href = 'https://news.walmart.com/events/walmarts-2018-open-call-event';
        }
        else if(window.location.href  == 'https://Certifications.walmart.com/') {
           window.location.href = 'https://Certifications.walmart.com/foundation';
        }
       else if(window.location.href  == 'https://corporate.walmart.com/20l8grr') {
           window.location.href = 'https://corporate.walmart.com/2018grr';
        }
        else if(window.location.href  == 'https://Certifications.walmart.com/The OSI Model Demystified') {
           window.location.href = 'https://Certifications.walmart.com/walmart-foundation/The OSI Model Demystified';
        }else if(window.location.href  == 'https://Certifications.walmart.com/Internetworking Basics') {
           window.location.href = 'https://Certifications.walmart.com/walmart-foundation/Internetworking Basics';
        }else if(window.location.href  == 'https://Certifications.walmart.com/Introduction to LAN Protocols') {
           window.location.href = 'https://Certifications.walmart.com/walmart-foundation/Introduction to LAN Protocols ';
        }
  })();
    </script>

