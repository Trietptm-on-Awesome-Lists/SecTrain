    <script>
        require(['/assets-bsp3/js/config.js'], function () {
            require(['jquery'], function ($) {
                $(document).ready(function () {



                    // fixes WAL-2955 and WAL-2954
                    if ($('.package-grid').length) {
                        var el = $('.package-grid'),
                       url = window.location.pathname,
                       newId = url.replace(/[^a-z0-9\s]/gi, '').replace(/[_\s]/g, '');
                        el.attr('id', newId);
                        // fixes WAL-2954
                        if (newId == "greenlightavet") {
                            $("#" + newId).find('.package-banner').wrap('<a href="http://www.greenlightavet.com/" target="_blank"></a>');

                    }
                    // special promo page fix
                    if ($('.special-page-lede').children('style').length) {
                        var style = $('.special-page-lede').children('style').text();
                        style = style.replace('!important', '');
                        style = style.replace(/;/g, ' !important;');
                        $('.special-page-lede').children('style').text(style);
                    }


                    // search fix
                    if ($('#flyoutSearch').size()) {
                        $('#flyoutSearch form.main-nav-search').attr('method', 'GET');
                    }
                    if ($('#landing-header').size()) {
                        $('#landing-header form.landing-header-search').attr('method', 'GET');
                    }

                    // walmart foundation fix - package v2
                    if ($('#package_header').size()) {
                        var headerlogo = $('#package_header').find('.package-header-banner').attr('src');
                        $('.package-header-mobile .package-header-mobile-logo img.global-header-logo').attr('src', headerlogo);
                    }


                    // Podcast page fixes.
                    $('.pv3-dl-inner-episode').each(function () {
                        $($(this).find('.pv3-dl-banner-episode img')[1]).prop('src', 'https://cdn.corporate.walmart.com/ef/8f/5e121664499d8fc3094327f17ee3/icon-ep-thick.png');
                    });
                    $('.pv3-footer-logo a').prop('target', '');
                    $('.pv3-footer-logo a').prop('href', 'https://corporate.walmart.com');


                    // Podcast expisode fix (reverse the order of the episode  numbers)
                    if ($('.Podcast-header ul.Podcast-overlay-episodes').length) {
                        numItems = $('.Podcast-header ul.Podcast-overlay-episodes li').length;
                        if ($('.Podcast-header ul.Podcast-overlay-episodes li:contains("Teaser")').length) {
                            numItems--;
                        }
                        $('.Podcast-header ul.Podcast-overlay-episodes li').each(function () {
                            $childSpan = $(this).find('span');
                            if ($childSpan.length) {
                                $childSpan.empty().append(numItems--);
                            }
                        })
                    }


                    // JS: Nov 14 2017 - Hide all the infoboxes which have started to appear on our map
                    setTimeout(function () {
                        $('div.MicrosoftMap #InfoboxCustom').each(function () { $(this).parent().css("display", "none"); });
                    }, 1500);

                    // JS fix - 24may 2018 - SparkletDL
                    if ($('[data-bsp-sparkletdl]').length) {

                        $('[data-bsp-sparkletdl] [data-dl-slider]').on('beforeChange', function (event, slick, currentSlide, nextSlide) {

                            setTimeout(() => {
                                console.log(slick);
                                if (nextSlide === 0) {
                                    var $slider = $('[data-bsp-sparkletdl] [data-dl-slider]');
                                    var $currentSlide = $slider.find('[data-slick-index="' + nextSlide + '"]');
                                    var $nextSlide = $slider.find('[data-slick-index="' + (parseInt(nextSlide) + 1) + '"]');
                                    var $prevSlide = $slider.find('[data-slick-index="' + (slick.$slides.length - 1) + '"]');

                                    console.log($prevSlide);

                                    var slideText = $currentSlide.find('[data-slide-text]').html();
                                    $('[data-bsp-sparkletdl] [data-current-text]').fadeOut(100, function () {
                                        $('[data-bsp-sparkletdl] [data-current-text]').empty().html(slideText).fadeIn();
                                    });

                                    if ($prevSlide.length) {
                                        var prevTitle = $prevSlide.find('[class*="__headline"]').text();

                                        slick.$prevArrow.stop().fadeOut(100, function () {
                                            slick.$prevArrow.text(prevTitle).fadeIn();
                                        });

                                    } else {
                                        slick.$prevArrow.empty().hide();
                                    }

                                    if ($nextSlide.length) {
                                        var nextTitle = $nextSlide.find('[class*="__headline"]').text();
                                        // slick.$nextArrow.text(nextTitle).show();
                                        slick.$nextArrow.stop().fadeOut(100, function () {
                                            slick.$nextArrow.text(nextTitle).fadeIn();
                                        });
                                        $('[data-bsp-sparkletdl] [data-dl-nav-nexttitle]').fadeOut(100, function () {
                                            $('[data-bsp-sparkletdl] [data-dl-nav-nexttitle]').text(nextTitle).fadeIn();
                                        });
                                    } else {
                                        slick.$nextArrow.empty().hide();
                                        $('[data-bsp-sparkletdl] [data-dl-nav-nexttitle]').empty().hide();
                                    }

                                }
                            }, 10)
                        });
                    }

                    // starts fix for WMT-374 - 05/16/2018
                    if ($('.PodcastV2-overlay .PodcastV2-overlay-items-item a[href*="http"]').length) {
                        $('.PodcastV2-overlay .PodcastV2-overlay-items-item a[href*="http"]').off('click')
                    }

                    // start fix for map label - WMT-413
                    if ($('.map-controls-country label[for="country"]').length) {
                        var text = $('.map-controls-country label[for="country"]').text();
                        text = text.replace('country', 'location');
                        $('.map-controls-country label[for="country"]').text(text);
                    }

                    //fixes modal on GRR
                    $('#grrModal').on('show.bs.modal', function () {
                        $('html, body').removeClass('modal-open')
                    });

                    $('#grrModal').on('hide.bs.modal', function () {
                        $('html, body').removeClass('modal-open')
                    });

                    /* Aug 16 - hide watch live event button */
                    if ($('h1.article-header-title').text() !== "FY2019 Q2 Earnings Release") {
                        $('.article-lead-media-block-inner a.btn').show();
                    }

                });
            });
        });

    </script>


