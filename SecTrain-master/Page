    <script type="text/javascript">
       (function () {
            //Check if Myriad-pro font is loaded
            var fontname = 'Myriad-pro';
            FontDetect.onFontLoaded (fontname, onFontLoaded, onFontDidntLoad, {msTimeout: 4000});

            function onFontLoaded (fontname){
                $('#main-nav .nav-item').css('font-size','13px');
            }
            function onFontDidntLoad (fontname) {
                $('#main-nav .nav-item').css('font-size','12px');
            }
        });
       (function () {
            var h = document.getElementsByTagName("html")[0];
            h.className += " wf-loading";
            var t = setTimeout(function () {
                h.className = h.className.replace(/(\s|^)wf-loading(\s|$)/g, " ");
                h.className += " wf-inactive"
            }, config.scriptTimeout);
            var tk = document.createElement("script"), d = false;
            tk.src = '//use.typekit.net/' + config.kitId + '.js';
            tk.type = "text/javascript";
            tk.async = "true";
            tk.onload = tk.onreadystatechange = function () {
                var a = this.readyState;
                if (d || a && a != "complete" && a != "loaded")return;
                d = true;
                clearTimeout( t );
                try {
                    Typekit.load(config)
                } catch ( b ) {
                }
            };
            var s = document.getElementsByTagName("script")[0];
            s.parentNode.insertBefore(tk, s)
        })();
    </script>
    <script type="text/javascript">
            function headerScroll() {
                var header = document.getElementById("landing-header"),
                    globHead = document.getElementById("global-corp-header"),
                    top = window.getScrollOffsets().y;
                    if( header ){
                        var  headerH = header.offsetHeight;
                        if (top <= headerH) {
                            header.className = header.className.replace(/\bscrolled\b/,'');

                            if (document.documentElement.className.indexOf('ie8') != -1) {
                                if(globHead.className.indexOf('noshow') === -1){
                                    globHead.className += ' noshow' ;
                                }
                            }
                        } else {
                            if(header.className.indexOf('scrolled') === -1){
                                header.className += ' scrolled';
                            }

                            if (document.documentElement.className.indexOf('ie8') != -1) {
                                globHead.className = globHead.className.replace(/\bnoshow\b/,'');
                            }
                        }
                    }
            }
            function getScrollOffsets() {
                // This works for all browsers except IE versions 8 and before
                if ( window.pageXOffset != null )
                   return {
                       x: window.pageXOffset,
                       y: window.pageYOffset
                   };

                // For browsers in Standards mode
                var doc = window.document;
                if ( document.compatMode === "CSS1Compat" ) {
                    return {
                        x: doc.documentElement.scrollLeft,
                        y: doc.documentElement.scrollTop
                    };
                }

                // For browsers in Quirks mode
                return {
                    x: doc.body.scrollLeft,
                    y: doc.body.scrollTop
                };
            }
            window.onscroll = headerScroll;
    </script>
